package software_test_5;

import org.openqa.selenium.By;
import org.openqa.selenium.WebDriver;
import org.openqa.selenium.WebElement;
import org.openqa.selenium.chrome.ChromeDriver;
import org.openqa.selenium.support.ui.Select;

import java.util.List;

public class Program_5 {

	public static void main(String[] args) {
		System.setProperty("webdriver.chrome.driver", "C:\\Users\\ISE2019\\eclipse-workspace\\software_test_4\\driver\\chromedriver.exe");
		WebDriver d = new ChromeDriver();
		d.get("C:\\Users\\ISE2019\\Desktop\\list.html");
		WebElement dropdown = d.findElement(By.xpath("//select"));
		Select s = new Select(dropdown);

		List<WebElement>options = s.getOptions();
		System.out.println("No. of Options :"+options.size() );

	}

}
